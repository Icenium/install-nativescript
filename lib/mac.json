{
    "getNodeCommand": "which node",
    "getNPMCommand": "which npm",
    "nodeInstallerSuffix": ".pkg",
    "nodeDownloadAddress": "https://nodejs.org/dist/v%{0}/node-v%{0}.pkg",
    "installCommand": "installer -pkg %{0}  -target /",
    "removeGlobalPackageCommand": "npm uninstall -g %{0}",
    "getCLICommand": "which nativescript",
    "installCLICommand": "npm install -g nativescript%{0} --ignore-scripts",
    "getPATHCommand": "",
    "getPrefixPathCommand": "npm config get prefix",
    "getCachePathCommand": "npm config get cache",
    "getCurrentUserCommand": "whoami",
    "fixNPMPermissionsCommand": "chown -R %{0} %{1}"
}
